<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.Shadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MenuController">
   <children>
   
            <ImageView>
         <Image url="@../images/bg.jpg" />
      </ImageView>
   
         <ImageView cache="true" fitHeight="159.0" fitWidth="222.0" layoutX="251.0" layoutY="80.0" preserveRatio="true">
         <Image url="@../images/PlayButton_1.png" />
      </ImageView>
   
         <ImageView fitHeight="165.0" fitWidth="222.0" layoutX="125.0" layoutY="196.0" preserveRatio="true">
         <Image url="@../images/QuitButton.png" />
      </ImageView>
   
         <Button fx:id="playGame" alignment="CENTER" layoutX="296.0" layoutY="109.0" mnemonicParsing="false" onAction="#handleClick" opacity="0.01" prefHeight="80.0" prefWidth="135.0" text="Play" textAlignment="CENTER">
         <effect>
            <Shadow />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   
         <Button fx:id="endGame" alignment="CENTER" layoutX="165.0" layoutY="228.0" mnemonicParsing="false" onAction="#handleClick" opacity="0.01" prefHeight="84.0" prefWidth="140.0" text="Quit" textAlignment="CENTER">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Shadow />
         </effect>
      </Button>
   </children>
</AnchorPane>
